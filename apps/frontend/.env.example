# API Configuration
VITE_API_URL=http://localhost:3001/api/v1
VITE_APP_NAME=CounselFlow Ultimate V4.1
VITE_APP_VERSION=4.1.0

# AI Provider API Keys
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_DEEPSEEK_API_KEY=your_deepseek_api_key_here
VITE_OLLAMA_URL=http://localhost:11434
VITE_GEMINI_API_KEY=your_gemini_api_key_here
VITE_CLAUDE_API_KEY=your_claude_api_key_here

# Environment
VITE_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true

# Session Configuration
VITE_SESSION_TIMEOUT_HOURS=24
VITE_REMEMBER_ME_DAYS=30

# Logging Configuration
VITE_LOG_LEVEL=debug
VITE_ENABLE_REMOTE_LOGGING=false
VITE_REMOTE_LOG_ENDPOINT=/api/logs

# Security
VITE_ENABLE_CSP=true
VITE_SECURE_COOKIES=true

# Demo Mode
VITE_DEMO_MODE=true
VITE_DEMO_AUTO_LOGIN=false